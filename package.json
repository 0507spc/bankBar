{
  "name": "bankbar",
  "version": "0.7.3",
  "description": "An Electron based menubar application",
  "license": "MIT",
  "author": "John Easton",
  "repository": "https://github.com/johneas10/bankBar",
  "devDependencies": {
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "electromon": "1.0.10",
    "electron": "3.0.6",
    "electron-builder": "20.19.2",
    "eslint": "4.19.1",
    "eslint-config-prettier": "3.3.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "8.0.0",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-standard": "4.0.0",
    "fetch-mock": "6.3.0",
    "husky": "1.0.0-rc.8",
    "jest": "22.4.3",
    "standard-version": "4.3.0",
    "prettier": "1.15.3"
  },
  "dependencies": {
    "debug": "3.1.0",
    "dotenv": "6.0.0",
    "electron-store": "1.3.0",
    "form-data": "2.3.2",
    "node-fetch": "2.1.2"
  },
  "main": "main.js",
  "scripts": {
    "dist": "electron-builder",
    "start": "electron .",
    "start:debug": "DEBUG=* electron .",
    "start:watch": "electromon .",
    "test": "jest",
    "test:watch": "jest --watch",
    "release": "standard-version",
    "publish": "git push --follow-tags origin master ",
    "commit": "git-cz",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "build": {
    "appId": "io.johneas.bankbar",
    "mac": {
      "icon": "app/images/icon-main.png"
    },
    "protocols": [
      {
        "name": "bankbar",
        "schemes": [
          "bankbar"
        ]
      }
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && npm run lint"
    }
  }
}
